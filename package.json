{
  "name": "portrait-menu-bar-app",
  "productName": "Portrait",
  "version": "0.0.6",
  "description": "An application to interact with Portrait",
  "main": "./dist/main.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-electron": "electron ./dist/main.mjs",
    "build": "tsc && npm run copy-assets",
    "build-windows": "tsc && npm run copy-assets-windows",
    "delete": "sudo rm -rf ./dist",
    "copy-assets": "cp -r ./src/assets ./src/contracts ./dist/",
    "copy-assets-windows": "robocopy ./src/assets ./dist/assets /E && robocopy ./src/contracts ./dist/contracts /E",
    "build-and-start-electron": " npm run build && npm run start-electron",
    "build-and-start-electron-windows": "npm run build-windows && npm run start-electron",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "ryan",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-wix": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@types/dockerode": "^3.3.23",
    "electron": "^30.0.1",
    "electron-wix-msi": "^5.1.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@waku/message-hash": "^0.1.15-08fc2d1.0",
    "@waku/sdk": "^0.0.27-08fc2d1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dockerode": "^4.0.0",
    "dotenv": "^16.4.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "ethers": "^6.12.1",
    "express": "^4.19.2",
    "multiformats": "^13.1.3",
    "nanoid": "^5.0.4",
    "node-fetch": "^3.3.2",
    "protobufjs": "^7.3.2",
    "update-electron-app": "^3.0.0"
  }
}
